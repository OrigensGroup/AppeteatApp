{
  "main": "index.js",
  "scripts": {
    "android": "set APP_ENV=staging react-native run-android",
    "ios": "APP_ENV=staging react-native run-ios",
    "start": "APP_ENV=staging react-native start --reset-cache",
    "web": "expo start --web",
    "type-check": "tsc --noEmit",
    "lint": "yarn lint:js yarn lint:css",
    "lint:fix": "yarn lint:js --fix",
    "lint:js": "eslint src --ext ts,tsx,js",
    "lint:css": "stylelint './src/**/styles.ts' --aei",
    "jest": "jest --silent",
    "test": "yarn jest --coverage",
    "test:watch": "yarn jest --watch",
    "test:coverage": "yarn jest --coverage",
    "prepare": "husky install",
    "build:ios": "APP_ENV=prod react-native run-ios --configuration Release",
    "build:android": "APP_ENV=prod react-native run-android --configuration Release",
    "clean": "yarn clean:ios && yarn clean:android",
    "clean:ios": "cd ios && pod cache clean --all && rm -rf build && cd ..",
    "clean:android": "cd android && ./gradlew cleanBuildCache && cd ..",
    "publish:staging": "APP_ENV=staging expo publish --release-channel staging --target bare",
    "publish:prod": "APP_ENV=staging expo publish --release-channel staging --target bare"
  },
  "dependencies": {
